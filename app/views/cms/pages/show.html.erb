<div id="main">
  <% if @page.is_displayed_title? %>
  <div class="default-title">
    <h1><%= @page.title %></h1>
  </div>
  <% end %>
  <div class="content">
    <%=raw @page.content %>
  </div>

  <a class="free-resume-builder-banner" href="http://resumecompanion.com" title="Free Resume Builder">Free Resume Builder</a>
</div>

<div id="sidebar">
  <%=raw @page.sidebar.try(:content) %>
</div>